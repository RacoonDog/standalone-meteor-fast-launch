{
  "schemaVersion": 1,
  "id": "fastlaunch",
  "version": "${version}",
  "name": "Standalone Meteor Fast Launch",
  "authors": [
    "Crosby"
  ],
  "contact": {
    "sources": "https://github.com/RacoonDog/standalone-meteor-fast-launch",
    "repo": "https://github.com/RacoonDog/standalone-meteor-fast-launch",
    "issues": "https://github.com/RacoonDog/standalone-meteor-fast-launch/issues"
  },
  "icon": "assets/fastlaunch/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "io.github.racoondog.fastlaunch.FastLaunch"
    ]
  },
  "mixins": [
    "fastlaunch.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  },
  "breaks": {
    "electron": "*"
  }
}
